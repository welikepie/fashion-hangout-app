<?xml version="1.0" encoding="UTF-8" ?>
<Module>
	<ModulePrefs title="Fashion Hangout App">
		<Require feature="rpc" />
		<Require feature="views" />
		<Require feature="locked-domain" />
	</ModulePrefs>
	<Content type="html"><![CDATA[

<!DOCTYPE html>
<html>
	<head>
		<title>Fashion Hangout App</title>
		<base href="https://raw.github.com/welikepie/fashion-hangout-app/v1/build/">
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
		<style type="text/css">html,body{width:100%;height:100%;padding:0;margin:0;font-family:Helvetica,Arial,sans-serif;font-size:12px;font-weight:normal;line-height:16px;color:#333}h1{padding:0;margin:0 0 16px}p{padding:0;margin:0 0 16px}.items{display:block;padding:0;margin:0;list-style:none}.main{position:relative;display:block;height:100%;margin:0 300px 0 0;overflow:auto}#playback{display:block;margin:0 16px 32px;overflow:hidden;background:#fff}#playback .video{display:block;width:600px;min-height:550px;margin:16px auto}#playback .video h1{margin:0;line-height:32px}#playback .video p{padding-bottom:7px;margin-bottom:8px;border-bottom:1px solid #CCC}#playback .video video{display:block;width:100%}#playback .playlist{position:relative;height:120px;padding:8px 0}#playback .playlist .items{height:124px;white-space:nowrap}#playback .playlist .items>*{display:inline-block;margin:0 8px;line-height:0;cursor:pointer;border:1px solid #CCC}#playback .playlist .items>* img{height:120px;border:1px solid #fff}#playback .playlist:before{position:absolute;top:0;left:0;z-index:2;width:48px;height:100%;background:-webkit-linear-gradient(0,#fff,#fff 33%,rgba(255,255,255,0));background:-moz-linear-gradient(0,#fff,#fff 33%,rgba(255,255,255,0));background:linear-gradient(0,#fff,#fff 33%,rgba(255,255,255,0));content:''}#playback .playlist:after{position:absolute;top:0;right:0;z-index:2;width:48px;height:100%;background:-webkit-linear-gradient(180deg,#fff,#fff 33%,rgba(255,255,255,0));background:-moz-linear-gradient(180deg,#fff,#fff 33%,rgba(255,255,255,0));background:linear-gradient(180deg,#fff,#fff 33%,rgba(255,255,255,0));content:''}#playback .playlist .controls *{position:absolute;top:40px;z-index:3;display:block;width:40px;height:40px;font-size:24px;font-weight:bold;line-height:40px;text-align:center;cursor:pointer;background:#999;border:1px solid white}#playback .playlist .controls .prev{left:40px}#playback .playlist .controls .next{right:40px}#catalogue{display:block;margin:0 316px 16px 16px}#catalogue .items{overflow:hidden}#catalogue .items>*{display:block;float:left;width:268px;min-height:240px;padding:16px;margin:16px;overflow:hidden;text-align:justify;background:#EEE}#catalogue .items>* img{float:left;max-width:80px;margin:0 8px 8px 0}#catalogue .items>* a{float:right;padding:3px 6px;font-size:14px;line-height:22px;cursor:pointer;background:#CCC}#wishlist{position:fixed;top:0;right:0;width:300px;height:100%;overflow:auto;border-left:1px solid #CCC}#wishlist .items>*{display:block;width:236px;padding:16px;margin:16px auto;overflow:hidden;text-align:justify;background:#EEE}#wishlist .items>* img{float:left;max-width:80px;margin:0 8px 8px 0}#wishlist .items>* a{float:right;padding:3px 6px;font-size:14px;line-height:22px;cursor:pointer;background:#CCC}</style>
	</head>
	<body>
		<div class="main">
			<section id="playback">

				<div class="video">
				
					<h1><!--!name!--></h1>
					<p><!--!description!--></p>
					<video controls poster="<!--!poster!-->">
						<!--% _.each(sources, function (val, key) { %-->
						<source type="<!--!key!-->" src="<!--!val!-->">
						<!--% }); %-->
					</video>
				
				</div>
				<div class="playlist">
				
					<ul class="items">
						<li>
							<img src="<!--!poster!-->" alt="<!--!name!-->">
						</li>
					</ul>
					
					<div class="controls">
						<a class="prev">&laquo;</a>
						<a class="next">&raquo;</a>
					</div>
				
				</div>

			</section>
			<section id="catalogue">

				<ul class="items">
				
					<li>
						<h1><!--!name!--></h1>
						<img src="<!--!photo!-->" alt="<!--!name!-->">
						<p><!--!description!--></p>
						<a class="add">Add to wishlist</a>
					</li>
				
				</ul>

			</section>
		</div>
		<section id="wishlist">

			<ul class="items">
			
				<li>
					<h1><!--!name!--></h1>
					<img src="<!--!photo!-->" alt="<!--!name!-->">
					<p><!--!description!--></p>
					<a class="remove">Remove from wishlist</a>
				</li>
			
			</ul>

		</section>
	</body>
	<script type="text/javascript" src="https://hangoutsapi.talkgadget.google.com/hangouts/api/hangout.js?v=1.3"></script>
	<script type="text/javascript" src="scripts/vendor.js"></script>
	<script type="text/javascript" src="scripts/scripts.js"></script>
</html>

	]]></Content>
</Module>